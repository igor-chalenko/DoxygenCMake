@PACKAGE_INIT@

get_filename_component(doxygen_dir ${CMAKE_CURRENT_LIST_FILE} PATH)
set_and_check(DOXYGEN_CMAKE_MODULE_DIR "${doxygen_dir}")
list(APPEND DOXYGEN_CMAKE_MODULE_PATH "${DOXYGEN_CMAKE_MODULE_DIR}")
list(APPEND CMAKE_MODULE_PATH "${DOXYGEN_CMAKE_MODULE_PATH}")

macro(_doxygen_find_package _name)
    find_package(${_name} ${ARGN})
endmacro()

include(${DOXYGEN_CMAKE_MODULE_DIR}/find-doxygen-cmake.cmake)

